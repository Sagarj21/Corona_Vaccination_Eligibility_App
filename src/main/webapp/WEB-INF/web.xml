<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <servlet>
  <servlet-name>vaccin</servlet-name>
  <servlet-class>com.nt.servlet.VaccinEligibilityServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>vaccin</servlet-name>
  <url-pattern>/coronaurl</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>welcome</servlet-name>
  <servlet-class>com.nt.servlet.ShowHomeServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>welcome</servlet-name>
  <url-pattern>/welcomeurl</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>corona</servlet-name>
  <jsp-file>/WEB-INF/pages/corona_vaccin.html</jsp-file>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>corona</servlet-name>
  <url-pattern>/crnaurl</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
  <welcome-file>crnurl</welcome-file>
  	<welcome-file>welcomeurl</welcome-file>
  	<welcome-file>corona_vaccin.jsp</welcome-file>
  </welcome-file-list>
  
  
</web-app>